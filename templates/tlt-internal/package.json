{
  "name": "@typelaunch-turbo/internal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "start": "vitest",
    "dev": "vitest",
    "test": "vitest run",
    "test:watch": "vitest",
    "cov": "vitest run --coverage",
    "cov:watch": "vitest run --coverage",
    "cov:detail": "vitest run --coverage && open-cli index.html",
    "lint": "tsc && eslint . --ext .ts && prettier --no-config --check 'src/**/*.ts'",
    "format": "eslint . --fix --ext .ts && prettier --no-config --write 'src/**/*.ts'",
    "uncache": "rm -rf .turbo",
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf node_modules"
  }
}
